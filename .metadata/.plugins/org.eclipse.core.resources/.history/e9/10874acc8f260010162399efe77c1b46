package com.test.spring.map;

import java.util.Map;

public class Test {
	
	private Map map;
	
	public Test(Map map)
	{
		this.map = map;
	}
	
	public void printMap(){
		if (map == null) System.out.println("map is null");
		if (map.isEmpty()) System.out.println("map is empty");
		System.out.println(map);
	}

}
